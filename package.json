  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "watch-js": "watchify -v -t [ reactify --es6 ] --debug clientapp -o public/bundle.js",
    "watch-css": "nodemon -q -e less -w clientapp/styles --exec \"lessc -x --source-map --source-map-less-inline --source-map-map-inline --source-map-rootpath=`pwd`/clientapp/styles clientapp/styles/index.less\" public/bundle.css",
    "watch-node": "nodemon -q --ignore public --ignore clientapp",
    "start": "npm run watch-js & npm run watch-css & npm run watch-node"
